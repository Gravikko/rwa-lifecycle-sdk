{
  "name": "@rwa-lifecycle/cli",
  "version": "1.0.0",
  "description": "Command-line interface for RWA Lifecycle SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "rwa": "./dist/index.js",
    "rwa-cli": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "cli": "node ./dist/index.js"
  },
  "dependencies": {
    "@rwa-lifecycle/core": "workspace:*",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0",
    "@types/inquirer": "^9.0.7"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rwa",
    "cli",
    "mantle",
    "bridge",
    "ethereum",
    "l2"
  ]
}
