{
  "name": "@rwa-lifecycle/core",
  "version": "1.0.0",
  "description": "Core SDK orchestrator for RWA Lifecycle",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@rwa-lifecycle/bridge": "workspace:*",
    "@rwa-lifecycle/gas": "workspace:*",
    "@rwa-lifecycle/indexer": "workspace:*",
    "@rwa-lifecycle/compliance": "workspace:*",
    "@rwa-lifecycle/storage": "workspace:*",
    "viem": "^2.21.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
